{
    "name": "sm_client_template",
    "zipFileName": "SM_Client_Template",
    "version": "0.0.0",
    "description": "HungryBear Gaming Slotmasters client_template",
    "main": "game.js",
    "scripts": {
        "start": "webpack-dev-server --host 0.0.0.0 --config ./buildCfg/webpack.config.dev.js --ui=slotmasters",
        "build": "webpack --mode production --config ./buildCfg/webpack.config.prod.js --ui=slotmasters && node distSize.mjs && node assetCheck.mjs",
        "buildWithTweaker": "webpack --mode production --config ./buildCfg/webpack.config.prod.js --tweaker=true --ui=slotmasters && node distSize.mjs && node assetCheck.mjs",
        "deploy": "cd ./deploy && node deploytoremoteSSH.js",
        "deployLocal": "cd ./deploy && node deploytoserver.js",
        "startIOS": "webpack-dev-server --host 0.0.0.0 --config ./buildCfg/webpack.config.dev.js --iosBuild=true --ui=slotmasters",
        "buildIOS": "webpack --mode production --config ./buildCfg/webpack.config.prod.js --iosBuild=true --ui=slotmasters && node distSize.mjs && node assetCheck.mjs",
        "distSize": "node distSize.mjs",
        "assetCheck": "node assetCheck.mjs"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "standard-version": {
        "skip": {
            "changelog": false,
            "commit": true,
            "tag": true,
            "bump": false
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hungrybeargaming/SM_WhatHappensInVegas.git"
    },
